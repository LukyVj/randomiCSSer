{
  "name": "randomicsser",
  "version": "1.0.8",
  "description": "A function that returns random numbers as css variables",
  "main": "./lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "require": "./lib/index.cjs",
    "default": "./lib/index.modern.js"
  },
  "module": "./lib/index.module.js",
  "unpkg": "./lib/index.umd.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "microbundle && cp -R lib demo",
    "dev": "microbundle watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukyVj/randomiCSSer.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LukyVj/randomiCSSer/issues"
  },
  "homepage": "https://github.com/LukyVj/randomiCSSer#readme",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "fs": "^0.0.1-security",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "jsdom": "^20.0.3",
    "randomicsser": "^1.0.1"
  }
}
